    "vscode-path-intellisense": "https://open-vsx.org/api/christian-kohler/path-intellisense/2.8.4/file/christian-kohler.path-intellisense-2.8.4.vsix",
    "vscode-eslint": "https://open-vsx.org/api/dbaeumer/vscode-eslint/2.4.0/file/dbaeumer.vscode-eslint-2.4.0.vsix",
    "vscode-prettier": "https://open-vsx.org/api/esbenp/prettier-vscode/9.10.4/file/esbenp.prettier-vscode-9.10.4.vsix",
    "vscode-gitlens": "https://open-vsx.org/api/eamodio/gitlens/2023.7.2205/file/eamodio.gitlens-2023.7.2205.vsix"
