{
  "name": "codevibeai-theia",
  "version": "0.1.0",
  "description": "CodeVibeAI - An emotionally intelligent vibe coding IDE",
  "keywords": [
    "theia-extension",
    "ai",
    "coding-assistant",
    "codevibeai"
  ],
  "homepage": "https://github.com/CodeVibeAI/theia",
  "bugs": {
    "url": "https://github.com/CodeVibeAI/theia/issues"
  },
  "license": "EPL-2.0 OR GPL-2.0-only WITH Classpath-exception-2.0",
  "author": "CodeVibeAI Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeVibeAI/theia.git"
  },
  "engines": {
    "node": ">=14.18.0",
    "yarn": ">=1.7.0 <2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@theia/cli": "1.42.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "circular-dependency-plugin": "^5.2.2",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "husky": "^9.0.11",
    "lerna": "^6.0.3",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "~5.4.5",
    "url-loader": "^4.1.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "prepare": "lerna run prepare && husky",
    "postinstall": "theia check:theia-version",
    "clean": "rimraf dist && theia clean && yarn clean:dist && yarn clean:packages",
    "clean:dist": "rimraf dist",
    "clean:packages": "lerna run clean",
    "build": "yarn build:dev",
    "build:dev": "cross-env NODE_ENV=development yarn build:extensions && yarn build:browser:dev && yarn build:electron:dev",
    "build:prod": "cross-env NODE_ENV=production yarn build:extensions && yarn build:browser:prod && yarn build:electron:prod",
    "build:extensions": "lerna run build --scope \"@codevibeai/*\" --stream --parallel",
    "build:browser:dev": "cross-env NODE_ENV=development theia rebuild:browser && yarn build:browser:bundle:dev",
    "build:browser:prod": "cross-env NODE_ENV=production theia rebuild:browser && yarn build:browser:bundle:prod",
    "build:browser:bundle:dev": "cross-env NODE_ENV=development theia build --app-target=browser",
    "build:browser:bundle:prod": "cross-env NODE_ENV=production theia build --app-target=browser",
    "build:electron:dev": "cross-env NODE_ENV=development theia rebuild:electron && yarn build:electron:bundle:dev",
    "build:electron:prod": "cross-env NODE_ENV=production theia rebuild:electron && yarn build:electron:bundle:prod",
    "build:electron:bundle:dev": "cross-env NODE_ENV=development theia build --app-target=electron",
    "build:electron:bundle:prod": "cross-env NODE_ENV=production theia build --app-target=electron",
    "analyze:bundle": "cross-env ANALYZE_BUNDLE=true yarn build:browser:bundle:prod",
    "watch": "lerna run watch --scope \"@codevibeai/*\" --parallel",
    "start": "yarn start:browser",
    "start:browser": "cross-env NODE_ENV=development yarn rebuild:browser && theia start --plugins=local-dir:plugins --port=3000",
    "start:electron": "cross-env NODE_ENV=development yarn rebuild:electron && theia start --plugins=local-dir:plugins --app-target=electron",
    "start:prod": "cross-env NODE_ENV=production yarn build:prod && cross-env NODE_ENV=production theia start --plugins=local-dir:plugins --port=3000",
    "watch:browser": "cross-env NODE_ENV=development yarn rebuild:browser --watch",
    "rebuild:browser": "theia rebuild:browser",
    "rebuild:electron": "theia rebuild:electron",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --config=jest.config.js --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --config=jest.config.js --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "publish:latest": "lerna publish --registry=https://registry.npmjs.org/ --no-git-tag-version --no-push",
    "publish:next": "lerna publish --registry=https://registry.npmjs.org/ --no-git-tag-version --no-push --exact --canary=next --yes",
    "update:next": "yarn upgrade -p \"@theia/.*\" --next",
    "package": "yarn build:prod && node scripts/package-app.js",
    "package:docker": "yarn build:prod && docker build -t codevibeai:latest ."
  },
  "dependencies": {
    "@theia/callhierarchy": "1.42.0",
    "@theia/core": "1.42.0",
    "@theia/debug": "1.42.0",
    "@theia/editor": "1.42.0",
    "@theia/editor-preview": "1.42.0",
    "@theia/file-search": "1.42.0",
    "@theia/filesystem": "1.42.0",
    "@theia/getting-started": "1.42.0",
    "@theia/git": "1.42.0",
    "@theia/keymaps": "1.42.0",
    "@theia/markers": "1.42.0",
    "@theia/messages": "1.42.0",
    "@theia/metrics": "1.42.0",
    "@theia/mini-browser": "1.42.0",
    "@theia/monaco": "1.42.0",
    "@theia/navigator": "1.42.0",
    "@theia/outline-view": "1.42.0",
    "@theia/output": "1.42.0",
    "@theia/plugin-dev": "1.42.0",
    "@theia/plugin-ext": "1.42.0",
    "@theia/plugin-ext-vscode": "1.42.0",
    "@theia/preferences": "1.42.0",
    "@theia/preview": "1.42.0",
    "@theia/process": "1.42.0",
    "@theia/property-view": "1.42.0",
    "@theia/scm": "1.42.0",
    "@theia/scm-extra": "1.42.0",
    "@theia/search-in-workspace": "1.42.0",
    "@theia/task": "1.42.0",
    "@theia/terminal": "1.42.0",
    "@theia/timeline": "1.42.0",
    "@theia/typehierarchy": "1.42.0",
    "@theia/toolbar": "1.42.0",
    "@theia/variable-resolver": "1.42.0",
    "@theia/vsx-registry": "1.42.0",
    "@theia/workspace": "1.42.0",
    "@codevibeai/core": "0.1.0",
    "@codevibeai/claude-integration": "0.1.0",
    "@codevibeai/context7-integration": "0.1.0",
    "@codevibeai/ui": "0.1.0",
    "@codevibeai/extension-system": "0.1.0",
    "axios": "^1.6.0",
    "keytar": "^7.9.0",
    "uuid": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "marked": "^5.0.0",
    "highlight.js": "^11.7.0"
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
